{
    "name": "smalot/magento-bundle",
    "description": "A Symfony2 wrapper for the Magento Soap Client API",
    "license": "MIT",
    "type":  "symfony-bundle",
    "authors": [
        {
            "name": "Sebastien Malot",
            "email": "sebastien@malot.fr"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.talisman-innovations.com"
        }
    ],
    "require": {
        "smalot/magento-client": "*",
        "php": ">=5.3.0",
        "talisman/connectorsupport": "dev-master",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/config": "^4.4|^5.0",
        "symfony/event-dispatcher": "^4.4|^5.0",
        "symfony/http-foundation": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0"
    },
    "require-dev": {
        "symfony/profiler-pack": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Smalot\\MagentoBundle\\": ""
        }
    },
    "minimum-stability": "stable",
    "target-dir": "Smalot/MagentoBundle"
}
